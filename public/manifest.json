{
  "manifest_version": 3,
  "name": "Minimal New Tab",
  "version": "1.0.0",
  "description": "A minimal and elegant new tab page with time, search, and quick bookmarks",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}